from pyproj import Proj
import numpy as np
import math

#goal
glon=139.753678
glat=35.688790

#現在地
lon=139.767120  #[deg.]
lat=35.681079  #[deg.]

#座標系にして距離と方位角をだす
grs80 = pyproj.Geod(ellps='GRS80')  # GRS80楕円体
shead, bkw_azimuth, dis = grs80.inv(lon, lat, glon, glat)

print(shead, bkw_azimuth, dis)


    
